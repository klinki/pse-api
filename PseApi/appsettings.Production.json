{
  "HttpConfiguration": {
    "UseHttpsRedirection": false
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.Loggly" ],
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "Serilog": "Information",
        "Inviser": "Information",
        "Inviser.Security.ApiKeyAuthenticationHandler": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Async",
        "Args": {
          "configure": [
            {
              "Name": "Loggly",
              "Args": {
                "customerToken": "",
                "tags": "pse"
              }
            }
          ]
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
    "Filter": [
      {
        "Name": "ByExcluding",
        "Args": {
          "expression": "StartsWith(@Message, 'Batch acquisition')"
        }
      }
    ],
    "Properties": {
      "Application": "PseApi"
    }
  }
}
